{
  "name": "accessibleolli",
  "version": "0.0.1",
  "description": "#AccessibleOlli demonstration infrastructure",
  "main": "index.js",
  "scripts": {
    "sim": "concurrently \"npm run telemetry\" \"npm run event_manager\" \"npm run monitor\" \"npm run cmdInterface\" \"npm run bestMile\" \"npm run olliAudioAgent\" \"npm run patronManager\" \"npm run agentProxy\"",
    "event_manager": "cd event_manager && npm start",
    "telemetry": "cd telemetry && npm start",
    "cmdInterface": "cd cmdline && npm start",
    "bestMile": "cd bestmile && cross-env NODE_CONFIG_DIR=../../../olli-monitor/config && node app.js",
    "olliAudioAgent": "cd olli_audio_agent && cross-env NODE_CONFIG_DIR=../../../olli-monitor/config && node app.js",
    "patronManager": "cd patron_manager && cross-env NODE_CONFIG_DIR=../../../olli-monitor/config && node app.js",
    "agentProxy": "cd agent_proxy && cross-env NODE_CONFIG_DIR=../../../olli-monitor/config && node app.js",
    "monitor": "cd monitor && cross-env NODE_CONFIG_DIR=../../../olli-monitor/config && npm start",
    "enable": "cd cmdline && node ao.js --control telemetry --operation enable",
    "disable": "cd cmdline && node ao.js --control telemetry --operation disable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<todo>.git"
  },
  "author": "pdykes@us.ibm.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pdykes/ao_sim/issues"
  },
  "homepage": "https://github.com/<todo>#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "child_process": "^1.0.2",
    "concurrently": "^3.5.1",
    "cross-spawn": "^5.1.0",
    "wait-on": "^2.0.2"
  },
  "devDependencies": {
    "cross-env": "^5.1.1"
  }
}
